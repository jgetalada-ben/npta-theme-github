.section--footer {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  background: linear-gradient(180deg, #17293f, #133051);

  padding: 80px 0 10px 0;
}

.footer-menu-column {
  width: 100%;
}

.footer-menu-title {
  color: #e9f24f !important;
  -webkit-text-fill-color: #e9f24f;
  font-family: Montserrat;
  font-size: 18px;
  font-style: normal;
  font-weight: 800;
  line-height: 104%; /* 18.72px */
  text-transform: uppercase;
}

.footer-menu-list {
  display: flex;
  vertical-align: middle;
  text-align: left;
  flex-wrap: wrap;
}
.footer-menu-list {
  list-style: none;
  margin: 0;
  padding: 0;
}

.footer-menu-list li {
  display: flex;
  align-items: center;
  width: 100%;
  gap: 10px;
}

.footer-menu-list a {
  position: relative;
  display: block;
  cursor: pointer;
  padding: 8px 0;
  color: #fff;
  text-decoration: none;
  color: #fff;
  font-family: Montserrat;
  font-size: 14px;
  font-style: normal;
  font-weight: 700;
  line-height: 150%; /* 21px */
  letter-spacing: 1.12px;
  text-transform: uppercase;
}
.footer-menu-list a:hover {
  color: #fff;
  text-decoration: none;
}

.footer-menu-list a:before,
a:after {
  content: "";
  position: absolute;
  width: 0%;
  height: 1px;
  bottom: -1px;
  background: #fff;
}

.footer-menu-list a:before {
  left: 0;
}
.footer-menu-list a:after {
  right: 0;
  background: #fff;
  transition: width 0.8s cubic-bezier(0.22, 0.61, 0.36, 1);
}

.footer-menu-list a:hover:before {
  background: #fff;
  width: 100%;
  transition: width 0.5s cubic-bezier(0.22, 0.61, 0.36, 1);
}

.footer-menu-list a:hover:after {
  background: transparent;
  width: 100%;
  transition: 0s;
}
.footer--social ul {
  list-style: none;
  padding: 0;
  display: flex;
  justify-content: flex-start;
  gap: 10px;
}
.footer--social ul li a {
  display: flex;
  justify-content: center;
  align-items: center;
}
.footer--social ul li a svg {
  width: 18px;
  height: 18px;
  fill: #fff;
}
.footer--social ul li:hover a svg {
  fill: #141414;
  transition: 300ms;
}
.footer--social ul li {
  border-radius: 3px;
  background: rgba(255, 255, 255, 0.12);
  display: inline-flex;
  justify-content: center;
  align-items: center;
  display: flex;
  width: 32px;
  height: 32px;

  justify-content: center;
  align-items: center;
}

.footer--social ul li:hover {
  transition: 300ms;
  background: #e9f24f;
}

.footer-settings-column ul {
  list-style: none;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  gap: 20px;
  padding: 0;
  flex-wrap: wrap;
}

.footer-settings-column ul li svg {
  max-height: 28px;
}

  @media only screen and (max-width: 768px) {
    .footer--second-row {
